{
  "name": "construction-safety-platform",
  "version": "1.0.1",
  "description": "基于AI视觉识别技术的建筑安全与质量检测平台 - 正式版本1.0.1",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node start.js",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "vercel-build": "cd client && npm install --legacy-peer-deps && npm run build",
    "install-client": "cd client && npm install --legacy-peer-deps",
    "install-all": "npm install && npm run install-client",
    "setup": "node start.js",
    "check": "node -e \"require('./start.js').checkEnvironment()\""
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.6",
    "axios": "^1.6.0",
    "mongoose": "^7.5.0",
    "dotenv": "^16.3.1",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.10.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "construction",
    "safety",
    "ai",
    "recognition",
    "building",
    "quality"
  ],
  "author": "Construction Safety Team",
  "license": "ISC"
}
