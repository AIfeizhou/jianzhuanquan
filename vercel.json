{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "https://jianzhuanquan-api.vercel.app/api/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
